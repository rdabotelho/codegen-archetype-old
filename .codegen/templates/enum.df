template {
	sourceFile: 'enum.ts'
	targetFile: 'src/enum/${domain.name}.ts'
	scope: 'enum'
	block(1, 1) {
		replace('DomainName', domain.name)
	}
	block(2, 2) {
		iterate(domain.attributes, item) {
            replace('NAME', item.name.upperCase)
            replace('name', item.name.lowerCase)
            delimiter('',',','')
		}
	}
}
